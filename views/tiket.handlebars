<div class="container">
  <div class="row">
    <div class="col-sm">
    </div>
<table class="table table-dark" id="mytable" style="margin-top: 10%">
      <thead>
        <tr>
          <th>Nama KA</th>
          <th>Pemberhentian</th>
          <th>Harga</th>
          <th>Pesan</th>
        </tr>
      </thead>
      <tbody>
         {{#each results}}
        <tr>
          <td>{{ namka }}</td>
          <td>{{ stasiun }}</td>
          <td>Rp. {{ harga }}</td>
          <td><a class="btn btn-sm btn-primary" href="/trims">Pesan</a></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  <div class="row">
    <div class="col-sm">
    </div>
</div>
